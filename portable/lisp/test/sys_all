#sh sys_test "(* 1 2 3 5)" "30"
#sh sys_test "(progn (+ 2 3) (* 3 4))" "12"
#sh sys_test "(defun test (x) (+ x 10) (* x 20)) (test 5)" "100"
#sh sys_test "(defvar a 10)(A)A" "10"
#sh sys_test "(defvar a 10) A" "10"
#sh sys_test "T NIL" "10"