echo "------------SYSTEM TEST-------------------"
sh sys_test "'a" "A"
sh sys_test "'()" "NIL"
sh sys_test "(quote())" "NIL"
sh sys_test "((lambda (x y) (cons x y)) 1 '(2))" "(1 2)"
sh sys_test "(defun null (x) (eq x (quote())))\
(null 'a) (null (quote ())) (null '())" "NULLNILTT"
sh sys_test "(defun f(x)\
         (cond \
            ((= x 1) 1) \
            (T (* x (f (- x 1))))))\
         (f 8) (f 3) (f 8) (f 9) (f 10) (f 10)" "F4790016006479001600"
sh sys_test "(defun f(x)\
         (cond \
            ((= x 1) 1) \
            (T (* x (f (- x 1))))))\
         (f 11) (f 11) (f 11) (f 10) (f 11)" "F399168003991680039916800362880039916800"
sh sys_test "(+ 1 2 3 4)" "10"
sh sys_test "(1" "expected )"
sh sys_test "(+ 1 2 3 5)" "11"
sh sys_test "(- 9 5 1 1)" "2"
sh sys_test "(* 1 2 3 5)" "30"
sh sys_test "(progn (+ 2 3) (* 3 4))" "12"
sh sys_test "(defun test (x) (+ x 10) (* x 20)) (test 5)" "100"
sh sys_test "(defvar a 10)(A)A" "10"
sh sys_test "(defvar a 10) A" "10"
sh sys_test "T NIL" "10"
sh sys_test "(defvar +bgr-index+ 0x1ce) \
(defvar +bgr-data+ 0x1cf) +BGR-INDEX+ +BGR-DATA+ X ABC" "+BGR-INDEX+ +BGR-DATA+"
sh sys_test "\"a b c d\""
sh sys_test "(defun f(x)\
             \"factorial f(x)\"\
         (cond \
            ((= x 1) 1) \
            (T (* x (f (- x 1))))))\
         (f 12) (f 10)" "F479001600"
sh sys_test "(setq test 200) test" "200"
sh sys_test "(defun bgr-out (reg data)\
  \"Запись регистра видео\"\
  (+ 0 reg)\
  (+ 1 data))\
\
(defun bgr-set-res (width height depth)\
  \"Установка разрешения экрана\"\
  (bgr-out 3 0)\
  (bgr-out 4 width)\
  (bgr-out 1 height)\
  (bgr-out 5 depth)\
  (bgr-out 3 1))\
\
(bgr-set-res 320 200 8)\
\
(defun graph-send-buffer (a) nil)\
\
(graph-send-buffer \"1234567890q\" )" ""
sh sys_test "(defvar str  \"1234567890111111qwertyu\\nuiop111\\n1111111111111111\\nqqqqqqqqqqqqqqqqqqqq\\nqqqqqqqqqqqqqqqqqqqqq\\nwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwww\") str" ""
sh sys_test "(setq a 1 b 2 c 3)a b c" "1 2 3"
sh sys_test "(setq a 1 b)" "1"
sh sys_test "(defvar a #(1 2 3)) a" "#(1 2 3)" 
#sh sys_test "(setq_rec a 1 b)" "1"