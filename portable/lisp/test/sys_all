echo "------------SYSTEM TEST-------------------"
sh sys_test "'a" "A"
sh sys_test "'()" "NIL"
sh sys_test "(quote())" "NIL"
sh sys_test "((lambda (x y) (cons x y)) 1 '(2))" "(1 2)"
sh sys_test "(defun null (x) (eq x (quote())))\
(null 'a) (null (quote ())) (null '())" "NULLNILTT"
sh sys_test "(defun f(x)\
         (cond \
            ((= x 1) 1) \
            (T (* x (f (- x 1))))))\
         (f 12) (f 3) (f 12)" "F4790016006479001600"
sh sys_test "(defun f(x)\
         (cond \
            ((= x 1) 1) \
            (T (* x (f (- x 1))))))\
         (f 13) " "F479001600"
